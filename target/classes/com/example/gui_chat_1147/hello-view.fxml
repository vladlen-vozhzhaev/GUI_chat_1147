<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" prefHeight="300" prefWidth="250" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.2" fx:controller="com.example.gui_chat_1147.HelloController">
   <children>
      <TextArea editable="false" prefHeight="275" prefWidth="250.0" fx:id="textArea" />
      <HBox prefHeight="25" prefWidth="250.0">
         <children>
            <TextField onAction="#handlerSend" prefWidth="170" fx:id="textField"/>
            <Button onAction="#handlerSend" mnemonicParsing="false" prefWidth="80" text="Отправить" />
         </children>
      </HBox>
   </children>
</VBox>